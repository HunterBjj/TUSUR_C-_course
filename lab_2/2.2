#include <iostream>

int main()
{
    std::cout<<"int: "<<sizeof(int)<<" bytes"<<std::endl;
    std::cout<<"unsigned: "<<sizeof(unsigned)<<" bytes"<<std::endl;
    std::cout<<"signed: "<<sizeof(signed)<<" bytes"<<std::endl;
